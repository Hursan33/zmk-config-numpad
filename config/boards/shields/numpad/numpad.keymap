    keymap {
        compatible = "zmk,keymap";

        default_layer {
            bindings = < 
            &to 2   &lt 1 RA(Q)
            &kp LS(N7)   &kp LS(N8)   &kp LS(N9)   &kp LS(LBKT)   &kp BACKSPACE
            &kp LS(N4)   &kp LS(N5)   &kp LS(N6)   &kp RA(SLASH)   &kp MINUS   &strieska
            &kp LS(N1)   &kp LS(N2)   &kp LS(N3)   &kp SLASH   &plus_minus
                &kp LS(N0)   &kp DOT   &kp N1   &kp COMMA   &kp ENTER
            >;
        };

        bt_layer {
            bindings = < 
            &none   &bootloader
            &sys_reset   &bt BT_SEL 0   &bt BT_SEL 1   &bt BT_SEL 2   &bt BT_CLR_ALL
            &none   &none   &none   &none   &none   &none
            &none   &none   &none   &none   &none
                &none   &none   &none   &none   &none
            >;
        };

        greek {
            bindings = < 
            &to 0   &alpha
            &betta   &delta   &epsilon   &eta   &kp BACKSPACE
            &theta   &kappa   &lambda   &mu   &nu   &xi
            &ro   &sigma   &tau   &pi   &phi
                &chi   &omikron   &psi   &omega   &kp ENTER
            >;
        };
    };
};