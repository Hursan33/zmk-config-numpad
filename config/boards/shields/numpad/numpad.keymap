        strieska: zed_em_kay {
            compatible = "zmk,behavior-macro";
            #binding-cells = <0>;
            bindings
                = <&macro_press &kp RALT>
                , <&macro_tap &kp N3>
                , <&macro_release &kp RALT>
                , <&macro_tap &kp SPACE>
                ;
        };
